@model IEnumerable<Proyecto1ED1.Models.Model.Cama>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumeroDeCama)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Disponible)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumeroDeCama)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Disponible)
        </td>
    </tr>
}

</table>
